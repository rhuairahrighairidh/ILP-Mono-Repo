{
  "name": "ilp-connector",
  "version": "21.2.0",
  "description": "ILP connector reference implementation",
  "repository": {
    "type": "git",
    "url": "git@github.com:interledgerjs/ilp-connector.git"
  },
  "keywords": [
    "interledger",
    "five-bells",
    "payments"
  ],
  "author": "Interledger Team <info@interledger.org>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/interledgerjs/ilp-connector/issues"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "start-prof": "node --prof --logfile=${CONNECTOR_V8_LOGFILE:-v8.log} src/index.js",
    "start:watch": "nodemon src/index.js",
    "build": "tsc --project .",
    "schema-to-tsd": "node scripts/build_schema_types",
    "prepare": "npm run schema-to-tsd && npm run build",
    "lint": "tslint --project .",
    "test": "istanbul test -- _mocha",
    "report-coverage": "codecov",
    "integration": "integration-loader && integration all",
    "bump": "version-bump"
  },
  "engines": {
    "node": ">=6.6.0"
  },
  "dependencies": {
    "ajv": "^5.5.2",
    "bignumber.js": "^5.0.0",
    "change-case": "^3.0.1",
    "debug": "^3.1.0",
    "extensible-error": "^1.0.2",
    "ilp-compat-plugin": "^2.0.3",
    "ilp-packet": "^2.1.2",
    "ilp-protocol-ildcp": "^1.0.0",
    "leveldown": "^2.1.1",
    "levelup": "^2.0.1",
    "lodash": "^4.17.4",
    "long": "^3.2.0",
    "node-fetch": "^1.7.3",
    "oer-utils": "^1.3.4",
    "reduct": "^3.2.0",
    "riverpig": "^1.1.3",
    "source-map-support": "^0.5.0",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/lodash": "^4.14.91",
    "@types/long": "^3.0.32",
    "@types/node": "^8.5.2",
    "@types/node-fetch": "^1.6.7",
    "@types/source-map-support": "^0.4.0",
    "@types/through2": "^2.0.33",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.0.0",
    "eslint": "^4.14.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "five-bells-integration-test-loader": "^1.4.0",
    "istanbul": "^0.4.5",
    "json-schema-to-typescript": "^5.2.2",
    "memdown": "^1.4.1",
    "mocha": "^4.1.0",
    "mock-require": "^2.0.2",
    "mock-socket": "^7.1.0",
    "nock": "^9.1.5",
    "nodemon": "^1.14.7",
    "sinon": "^4.1.3",
    "spec-xunit-file": "0.0.1-3",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.6.2"
  },
  "config": {
    "five-bells-integration-test-loader": {
      "module": "five-bells-integration-test",
      "repo": "interledgerjs/five-bells-integration-test"
    }
  }
}
