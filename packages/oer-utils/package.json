{
  "name": "oer-utils",
  "version": "1.3.4",
  "description": "Tools for OER parsing and serialization",
  "main": "dist/src/index.js",
  "typings": "dist/src/index",
  "directories": {
    "test": "test"
  },
  "files": [
    "index.js",
    "reader.js",
    "writer.js",
    "predictor.js",
    "dist/src"
  ],
  "scripts": {
    "lint": "tslint \"src/**/*.ts\" \"test/*.spec.ts\"",
    "build": "rm -rf dist ; tsc",
    "test": "npm run build && npm run test:specs",
    "test:specs": "mocha",
    "precover": "rm -rf .nyc_output",
    "cover": "nyc --extension .ts --reporter=text-summary --reporter=html mocha",
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/interledgerjs/oer-utils.git"
  },
  "keywords": [
    "canonical",
    "oer",
    "parsing",
    "binary",
    "buffer",
    "serialization",
    "deserialization",
    "asn1",
    "format"
  ],
  "author": "Interledger Team <info@interledger.org>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/interledgerjs/oer-utils/issues"
  },
  "homepage": "https://github.com/interledgerjs/oer-utils",
  "dependencies": {
    "bignumber.js": "^6.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.12",
    "chai": "^4.1.0",
    "codecov": "^2.2.0",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "tslint-config-standard": "^6.0.1",
    "typescript": "^2.4.1"
  }
}
